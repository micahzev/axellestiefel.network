{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "appAssociation": "AUTO",
  "rewrites": [
    {
      "source": "/link/**",
      "dynamicLinks": true
    }
  ]
}
